// Avenir

// Regular

@font-face {
  font-family: "avenir-next-web";
  src: url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.eot?#iefix") format("eot")
}

@font-face {
  font-family: "avenir-next-web";
  src: url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.eot?#iefix");
  src: url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.eot?#iefix") format("eot"),
       url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.woff2") format("woff2"),
       url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.woff") format("woff"),
       url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.ttf") format("truetype"),
       url("//cdn.auth0.com/styleguide/core/development/fonts/avenir-next/regular.svg#regular") format("svg");
  font-weight: normal;
  font-style: normal;
}

