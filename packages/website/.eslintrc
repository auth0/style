{
  "extends": "auth0/prettier",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": "off",
    "import/no-extraneous-dependencies": "off"
  },
  "env": {
    "browser": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "moduleDirectory": [
          "node_modules",
          "src"
        ]
      }
    }
  }
}